{
   "name": "deeeep.io",
   "productName": "Deeeep.io_Desktop_App",
   "version": "1.5.7",
   "author": "SirReadsALot",
   "description": "The Deeeep.io Desktop Client",
   "main": "src/main.js",
   "scripts": {
      "start": "electron .",
      "pack": "electron-builder --dir",
      "dist": "electron-builder",
      "mac": "electron-builder --mac"
   },
   "build": {
      "productName": "Deeeep.io_Desktop_Client",
      "appId": "io.deeeep.desktop",
      "compression": "normal",
      "files": [
         "package.json",
         "public/**",
         "src/**.js",
         "build/icon.ico",
         "build/splashIntro_resized.png",
         "build/installerIcon.ico",
         "build/icons/mac/icon.icns"
      ],
      "directories": {
         "buildResources": "build"
      },
      "nsis": {
         "oneClick": false,
         "allowToChangeInstallationDirectory": true,
         "createDesktopShortcut": true,
         "installerIcon": "build/installerIcon.ico",
         "uninstallerIcon": "build/installerIcon.ico",
         "runAfterFinish": false
      },
      "dmg": {
         "contents": [
            {
               "x": 110,
               "y": 150
            },
            {
               "x": 240,
               "y": 150,
               "type": "link",
               "path": "/Applications"
            }
         ],
         "icon": "build/icons/mac/icon.icns"
      }
   },
   "dependencies": {
      "discord-rpc": "^3.2.0",
      "electron-updater": "^4.3.9"
   },
   "devDependencies": {
      "electron": "^12.0.1",
      "electron-builder": "^22.10.5"
   }
}
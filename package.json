{
   "name": "deeeep.io.desktop.client",
   "version": "1.6.7",
   "author": "SirReadsALot",
   "description": "The Deeeep.io Desktop Client",
   "main": "src/main.js",
   "scripts": {
      "start": "electron .",
      "publish": "electron-builder -p always"
   },
   "build": {
      "publish": [
         {
            "provider": "github",
            "owner": "FeeshDev",
            "repo": "Deeeep.io-Desktop-Client"
         }
      ],
      "productName": "Deeeep.io Desktop Client",
      "appId": "com.github.FeeshDev.deeeepiodesktopclient",
      "compression": "normal",
      "files": [
         "package.json",
         "public/**",
         "src/**.js",
         "build/**"
      ],
      "directories": {
         "buildResources": "build"
      },
      "nsis": {
         "oneClick": false,
         "allowToChangeInstallationDirectory": true,
         "createDesktopShortcut": true,
         "installerIcon": "build/installerIcon.ico",
         "uninstallerIcon": "build/installerIcon.ico",
         "runAfterFinish": false
      }
   },
   "dependencies": {
      "discord-rpc": "^4.0.1",
      "electron-updater": "^4.3.9",
      "fs-extra": "^10.0.0"
   },
   "devDependencies": {
      "electron": "^12.0.0",
      "electron-builder": "^22.11.7"
   }
}